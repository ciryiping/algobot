"""
Please run this code from the algobot root dir.
"""
import pandas as pd
from algobot.databot.Google.FRDownloader import FRBatchDownloader

univ = pd.read_csv('data/2014/11/IWV_20141114.csv')

# Clean Data
inx1 = univ.Exchange == 'New York Stock Exchange Inc.'
inx2 = univ.Exchange == 'NASDAQ'
univ.Exchange[inx1] = 'NYSE'

tickerList = {}
for r in univ.iterrows():
    tickerList[r[1].Ticker] = r[1].Exchange

downloader = FRBatchDownloader()
